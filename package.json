{
  "name": "@careeros/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev:web": "pnpm run fix:hydration && pnpm -w exec nx run web:serve",
    "dev": "pnpm run dev:web",
    "smoke:trpc": "node tools/scripts/trpc-smoke.cjs",
    "ping:trpc": "node tools/scripts/ping-trpc.cjs",
    "build": "pnpm -w exec nx run-many -t build --all --exclude=root",
    "test": "pnpm -w test:api && pnpm -w test:web",
    "test:api": "vitest run --config apps/api/vitest.config.ts",
    "test:web": "vitest run --root web --config vitest.config.ts --pool forks",
    "test:all": "pnpm -w exec nx run-many -t test --all --skip-nx-cache --exclude=root",
    "release:prep": "pnpm -w tsc -b && pnpm -w exec nx run-many -t build --all --exclude=root && pnpm -w exec nx run-many -t test --all --skip-nx-cache --exclude=root && node tools/scripts/run-final-scan.cjs",
    "wire:trpc-web": "node tools/scripts/wire-trpc-web.cjs",
    "check:trpc": "node tools/scripts/check-trpc-wiring.cjs",
    "fix:trpc": "node tools/scripts/check-trpc-wiring.cjs --fix",
    "check:appshape": "node tools/scripts/check-application-shape.cjs",
    "try:tracker": "node tools/scripts/try-tracker-router-variants.cjs",
    "try:tracker:apply": "node tools/scripts/try-tracker-router-variants.cjs --apply-best",
    "maintain:all": "node tools/scripts/maintain-all.cjs",
    "maintain:all:ci": "node tools/scripts/maintain-all.cjs --commit",
    "phase3:tracker": "node tools/scripts/phase3-tracker.cjs",
    "verify:activity": "node tools/scripts/verify-activity.cjs",
    "dev:open-activity": "node tools/scripts/open-activity.cjs",
    "dev:activity": "node tools/scripts/dev-activity.cjs",
    "activity:ci": "node tools/scripts/seed-and-verify-activity.cjs --host http://localhost --port 3000 --strict --out activity-ci-report.json",
    "activity:ci:auto": "node tools/scripts/seed-and-verify-activity.cjs --host http://localhost --port 3000 --out activity-ci-report.json",
    "ci:web": "start-server-and-test \"cross-env NX_DAEMON=false pnpm -w exec nx run web:serve --port=3000\" http://localhost:3000 \"pnpm run activity:ci:auto\"",
    "ci:web:prod": "start-server-and-test \"pnpm -w exec nx run web:start\" http://localhost:3000 \"pnpm run activity:ci\"",
    "db:migrate": "pnpm -w exec prisma migrate deploy || pnpm -w exec prisma db push",
    "fix:phase3:tracker": "node tools/scripts/fix-phase3-tracker.cjs",
    "fix:phase3:all": "node tools/scripts/fix-phase3-all.cjs",
    "fix:summary": "node tools/scripts/fix-summary-once.cjs",
    "mark:stubs": "node tools/scripts/mark-existing-stubs.cjs",
    "scan:stubs": "node tools/scripts/scan-stubs.cjs",
    "lint:stubs": "node tools/scripts/enforce-no-stubs.cjs",
    "orchestrate:phase3": "node tools/scripts/orchestrate-phase3.cjs",
    "fix:phase4": "node tools/scripts/phase4-auto-fix-web.cjs",
    "fix:phase4:verify": "node tools/scripts/phase4-auto-fix-web.cjs --verify",
    "scan": "node tools/scripts/project-scan.cjs",
    "scan:fast": "node tools/scripts/project-scan.cjs --fast",
    "scan:smart": "node tools/scripts/project-scan.cjs --onlySmart",
    "scan:final": "node tools/scripts/project-scan.cjs --noLint --fast",
    "scan:final:legacy": "node tools/scripts/run-final-scan.cjs",
    "scan:pages": "node tools/scripts/scan-pages.cjs",
    "scan:pages:fast": "node tools/scripts/scan-pages.cjs --fast",
    "fix:pages": "node tools/scripts/fix-pages.cjs",
    "fix:pages:dry": "node tools/scripts/fix-pages.cjs --dry",
    "fix:pages:scan": "node tools/scripts/fix-pages.cjs --scan tools/reports/pages-scan-latest.json",
    "test:vitest": "node --max-old-space-size=4096 node_modules/.bin/vitest run",
    "check:activity": "node tools/scripts/runtime-activity-check.cjs",
    "ci:check:activity": "node tools/scripts/ci-check-activity.cjs",
    "test:web:ui": "vitest --root web --config vitest.config.ts",
    "fix:hydration": "node tools/scripts/ensure-body-suppressHydration.cjs"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@next/eslint-plugin-next": "^15.5.3",
    "@nx/devkit": "21.2.2",
    "@nx/eslint": "21.2.2",
    "@nx/eslint-plugin": "21.2.2",
    "@nx/express": "^21.2.2",
    "@nx/js": "21.2.2",
    "@nx/nest": "^21.2.2",
    "@nx/next": "^21.2.2",
    "@nx/node": "^21.2.2",
    "@nx/playwright": "21.2.2",
    "@nx/web": "21.2.2",
    "@nx/webpack": "21.2.2",
    "@nx/workspace": "21.2.2",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "0.2.39",
    "@tailwindcss/postcss": "4.1.13",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.6.1",
    "@trpc/client": "10.45.2",
    "@trpc/react-query": "10.45.2",
    "@trpc/server": "10.45.2",
    "@types/node": "22.18.1",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "5.0.3",
    "@vitejs/plugin-react-swc": "4.0.1",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "10.0.0",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "9.1.7",
    "jest-environment-node": "^29.7.0",
    "jsdom": "27.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "msw": "^2.11.2",
    "nx": "21.2.2",
    "postcss": "^8.5.6",
    "prettier": "^2.6.2",
    "prisma": "^6.15.0",
    "start-server-and-test": "^2.1.0",
    "tailwindcss": "^4.1.11",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "tsx": "4.20.5",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@prisma/client": "^6.15.0",
    "@tanstack/react-query": "4.41.0",
    "axios": "^1.6.0",
    "next": "~15.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "superjson": "2.2.2",
    "zod": "4.1.5"
  },
  "pnpm": {
    "overrides": {
      "@trpc/server": "10.45.2",
      "@trpc/client": "10.45.2",
      "@trpc/react-query": "10.45.2",
      "@trpc/next": "10.45.2",
      "@trpc/meta": "10.45.2"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "sharp"
    ]
  },
  "engines": {
    "node": ">=20 <23"
  }
}
